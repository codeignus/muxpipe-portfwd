{
	"name": "muxpipe-portfwd",
	"version": "0.1.0-pre",
	"repository": {
		"type": "git",
		"url": "https://github.com/codeignus/muxpipe-portfwd.git"
	},
	"author": "codeignus",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"type": "commonjs",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pnpm tsc",
		"clean": "rimraf dist node_modules",
		"format": "pnpm biome format --write",
		"format:check": "pnpm biome format",
		"prepare": "pnpm format:check && pnpm lint && pnpm build",
		"lint": "pnpm biome lint",
		"test": "vitest run",
		"test:watch": "vitest watch"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/node": "^24.8.0",
		"rimraf": "^6.0.1",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"pino": "^10.1.0",
		"yamux-js": "github:codeignus/yamux-js"
	}
}
